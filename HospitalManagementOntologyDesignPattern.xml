<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://www.semanticweb.org/healthcare-ontology#"
     xml:base="http://www.semanticweb.org/healthcare-ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">

    <owl:Ontology rdf:about="http://www.semanticweb.org/healthcare-ontology">
        <rdfs:comment>Healthcare Ontology for Doctors and Patients (HODP) - نظام الأنطولوجيا الشامل لإدارة الرعاية الصحية</rdfs:comment>
        <rdfs:label>HODP - Healthcare Ontology</rdfs:label>
        <owl:versionInfo>2.0</owl:versionInfo>
        <rdfs:comment>يتضمن جميع الأفراد من الأطباء، المرضى، المواعيد، والعلاجات</rdfs:comment>
    </owl:Ontology>

    <!-- الفئات الأساسية -->
    <owl:Class rdf:about="#Person"/>
    
    <owl:Class rdf:about="#Patient">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label>مريض</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#Doctor">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label>طبيب</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#MedicalEvent"/>
    
    <owl:Class rdf:about="#Appointment">
        <rdfs:subClassOf rdf:resource="#MedicalEvent"/>
        <rdfs:label>موعد</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#Treatment">
        <rdfs:subClassOf rdf:resource="#MedicalEvent"/>
        <rdfs:label>علاج</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#FinancialDocument"/>
    
    <owl:Class rdf:about="#Bill">
        <rdfs:subClassOf rdf:resource="#FinancialDocument"/>
        <rdfs:label>فاتورة</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#Organization"/>
    
    <owl:Class rdf:about="#InsuranceProvider">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>مزود تأمين</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#HospitalBranch">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>فرع مستشفى</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#MedicalSpecialization">
        <rdfs:label>تخصص طبي</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#PaymentMethod">
        <rdfs:label>طريقة دفع</rdfs:label>
    </owl:Class>

    <!-- فئات فرعية للمواعيد -->
    <owl:Class rdf:about="#ScheduledAppointment">
        <rdfs:subClassOf rdf:resource="#Appointment"/>
        <rdfs:label>موعد مجدول</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#CompletedAppointment">
        <rdfs:subClassOf rdf:resource="#Appointment"/>
        <rdfs:label>موعد مكتمل</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#CancelledAppointment">
        <rdfs:subClassOf rdf:resource="#Appointment"/>
        <rdfs:label>موعد ملغى</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#NoShowAppointment">
        <rdfs:subClassOf rdf:resource="#Appointment"/>
        <rdfs:label>موعد غير محضر</rdfs:label>
    </owl:Class>

    <!-- فئات للاستدلال -->
    <owl:Class rdf:about="#SeniorDoctor">
        <rdfs:subClassOf rdf:resource="#Doctor"/>
        <rdfs:label>طبيب مخضرم</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#ElderlyPatient">
        <rdfs:subClassOf rdf:resource="#Patient"/>
        <rdfs:label>مريض مسن</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#HighCostTreatment">
        <rdfs:subClassOf rdf:resource="#Treatment"/>
        <rdfs:label>علاج عالي التكلفة</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#DelinquentAccount">
        <rdfs:subClassOf rdf:resource="#Bill"/>
        <rdfs:label>حساب متأخر</rdfs:label>
    </owl:Class>

    <!-- خصائص الكائنات -->
    <owl:ObjectProperty rdf:about="#hasAppointment">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Appointment"/>
        <rdfs:label>لديه موعد</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#isAppointmentOf">
        <rdfs:domain rdf:resource="#Appointment"/>
        <rdfs:range rdf:resource="#Patient"/>
        <owl:inverseOf rdf:resource="#hasAppointment"/>
        <rdfs:label>موعد خاص بـ</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#supervisedBy">
        <rdfs:domain rdf:resource="#Appointment"/>
        <rdfs:range rdf:resource="#Doctor"/>
        <rdfs:label>مشرف عليه من قبل</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#supervises">
        <rdfs:domain rdf:resource="#Doctor"/>
        <rdfs:range rdf:resource="#Appointment"/>
        <owl:inverseOf rdf:resource="#supervisedBy"/>
        <rdfs:label>يشرف على</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#resultsIn">
        <rdfs:domain rdf:resource="#Appointment"/>
        <rdfs:range rdf:resource="#Treatment"/>
        <rdfs:label>يؤدي إلى</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#isResultOf">
        <rdfs:domain rdf:resource="#Treatment"/>
        <rdfs:range rdf:resource="#Appointment"/>
        <owl:inverseOf rdf:resource="#resultsIn"/>
        <rdfs:label>نتيجة لـ</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#generates">
        <rdfs:domain rdf:resource="#Treatment"/>
        <rdfs:range rdf:resource="#Bill"/>
        <rdfs:label>يولد</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#isGeneratedBy">
        <rdfs:domain rdf:resource="#Bill"/>
        <rdfs:range rdf:resource="#Treatment"/>
        <owl:inverseOf rdf:resource="#generates"/>
        <rdfs:label>مولد بواسطة</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#hasInsurance">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#InsuranceProvider"/>
        <rdfs:label>لديه تأمين</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#worksAt">
        <rdfs:domain rdf:resource="#Doctor"/>
        <rdfs:range rdf:resource="#HospitalBranch"/>
        <rdfs:label>يعمل في</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#hasSpecialization">
        <rdfs:domain rdf:resource="#Doctor"/>
        <rdfs:range rdf:resource="#MedicalSpecialization"/>
        <rdfs:label>متخصص في</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#paidBy">
        <rdfs:domain rdf:resource="#Bill"/>
        <rdfs:range rdf:resource="#PaymentMethod"/>
        <rdfs:label>مدفوع بواسطة</rdfs:label>
    </owl:ObjectProperty>

